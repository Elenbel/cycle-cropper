{
  "name": "cycle-cropper",
  "version": "1.1.16",
  "description": "Cycle.js component for cropper.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "webpack && dts-bundle --name cycle-pusher --baseDir dist --main dist/src/index.d.ts --out index.d.ts",
    "lint": "./node_modules/.bin/tslint -c tslint.json src/**/*.ts",
    "watch:lint": "watch 'npm run lint' src",
    "watch:build": "watch 'npm run build' src",
    "watch": "watch 'npm run lint && npm run build' src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unhappychoice/cycle-cropper.git"
  },
  "keywords": [
    "cropper.js",
    "crop",
    "image"
  ],
  "author": "unhappychoice",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unhappychoice/cycle-cropper/issues"
  },
  "homepage": "https://github.com/unhappychoice/cycle-cropper#readme",
  "dependencies": {
    "@types/cropperjs": "^1.1.0",
    "cropperjs": "^1.1.3"
  },
  "peerDependencies": {
    "@cycle/dom": "^19.*",
    "@cycle/run": "^3.*",
    "xstream": "^11.*"
  },
  "devDependencies": {
    "@cycle/dom": "^19.3.0",
    "@cycle/run": "^3.4.0",
    "css-loader": "^0.28.8",
    "extract-text-webpack-plugin": "^3.0.2",
    "ts-loader": "^3.2.0",
    "tslint": "^5.7.0",
    "typescript": "^2.6.2",
    "watch": "^1.0.2",
    "webpack": "^3.10.0",
    "xstream": "^11.*"
  }
}
